        "@codeceptjs/ui": "^0.4.6",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@types/jest": "^27.4.0",
        "babel-jest": "^27.4.6",
        "babel-loader": "^8.2.3",
