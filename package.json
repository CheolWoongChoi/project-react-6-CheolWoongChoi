{
    "name": "tmi",
    "version": "1.0.0",
    "description": "오늘의 메뉴를 알려주는 서비스",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=development webpack serve --mode=development",
        "test:unit": "jest --coverage",
        "test:e2e": "codeceptjs run --steps",
        "test": "npm run test:unit && npm run test:e2e",
        "lint": "eslint --ext js,jsx .",
        "build": "NODE_ENV=production webpack --mode=production",
        "deploy": "gh-pages -d dist -b release -m 'release 업데이트'",
        "codeceptjs": "codeceptjs run --steps",
        "codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
        "codeceptjs:ui": "codecept-ui --app",
        "codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples",
        "codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples",
        "codeceptjs:demo:ui": "codecept-ui --app  -c node_modules/@codeceptjs/examples"
    },
    "author": "CheolWoong Choi",
    "license": "ISC",
